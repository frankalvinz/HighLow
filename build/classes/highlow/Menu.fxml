<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="highlow.MenuController">
   <children>
      <Label layoutX="24.0" layoutY="333.0" prefHeight="30.0" prefWidth="100.0" text="Highscore: ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="highScore" editable="false" layoutX="125.0" layoutY="335.0" promptText="0" />
      <Label layoutX="72.0" layoutY="66.0" text="High - Low">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="newGameBtn" layoutX="100.0" layoutY="168.0" mnemonicParsing="false" onAction="#newGameBtnOnAction" prefHeight="40.0" prefWidth="100.0" text="New Game" />
      <Button fx:id="rulesBtn" layoutX="100.0" layoutY="243.0" mnemonicParsing="false" onAction="#rulesBtnOnAction" prefHeight="40.0" prefWidth="100.0" text="Rules" />
   </children>
</AnchorPane>
